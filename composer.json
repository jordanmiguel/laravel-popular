{
    "name": "JordanMiguel/laravel-popular",
    "type": "library",
    "description": ":package_description",
    "keywords": [
        "JordanMiguel",
        "laravel-popular",
        "Laravel",
        "Popular",
        "Laravel Popular"
    ],
    "homepage": "https://github.com/JordanMiguel/laravel-popular",
    "license": "MIT",
    "authors": [
        {
            "name": "Jordan Miguel",
            "homepage": "https://github.com/JordanMiguel",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : "~5.6|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=5.4.3",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "JordanMiguel\\LaravelPopular\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JordanMiguel\\LaravelPopular\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
